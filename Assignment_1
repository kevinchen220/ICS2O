PImage img;
void setup(){
  size(395,366);
  
  img = loadImage("unnamed.jpg");
  
}

void draw(){
   background(35,143,244);
   if (keyPressed){
   //image(img,0,0); 
   }
   //background pictures
   fill(45,173,246);
   
   //Star above left wing
   beginShape();
   vertex(42,77);
   vertex(53,71);
   vertex(66,47);
   vertex(90,60);
   vertex(119,60);
   vertex(119,75);
   vertex(68,89);
   endShape();
   
   //Star above rigth wing
   beginShape();
   vertex(290,77);
   vertex(304,71);
   vertex(317,47);
   vertex(340,60);
   vertex(369,60);
   vertex(369,72);
   vertex(331,87);
   endShape();
   
   //Shape under left wing
   beginShape();
   curveVertex(36,251);
   curveVertex(36,251);
   curveVertex(31,261);
   curveVertex(32,271);
   curveVertex(39,278);
   curveVertex(76,291);
   vertex(76,291);
   vertex(106,255);
   endShape();
   
   //Gun at bottom left
   beginShape();
   vertex(54,366);
   vertex(97,349);
   curveVertex(97,349);
   curveVertex(99,331);
   curveVertex(119,342);
   curveVertex(119,342);
   vertex(130,367);
   endShape();
   
   //Gun at bottom right
   beginShape();
   vertex(305,366);
   vertex(347,349);
   curveVertex(347,349);
   curveVertex(349,331);
   curveVertex(369,342);
   curveVertex(369,342);
   vertex(380,367);
   endShape();
   
   //Star under big star
   beginShape();
   vertex(156,321);
   vertex(164,336);
   vertex(163,363);
   vertex(192,363);
   vertex(195,366);
   vertex(223,366);
   vertex(231,353);
   vertex(254,339);
   vertex(244,320);
   endShape();
   
   //Shape under right wing
   beginShape();
   vertex(247,254);
   vertex(335,291);
   curveVertex(335,291);
   curveVertex(342,293);
   curveVertex(351,289);
   curveVertex(355,285);
   vertex(355,285);
   vertex(367,253);
   vertex(319,225);
   ellipse(363,238,32,32);
   endShape();
   
   //Gun on top of big star
   beginShape();
   vertex(168,35);
   vertex(165,28);
   curveVertex(165,29);
   curveVertex(163,28);
   curveVertex(160,26);
   curveVertex(157,23);
   curveVertex(157,19);
   curveVertex(157,17);
   curveVertex(159,15);
   curveVertex(161,15);
   curveVertex(163,16);
   curveVertex(166,17);
   curveVertex(171,14);
   curveVertex(172,12);
   vertex(171,7);
   vertex(186,0);
   vertex(249,0);
   vertex(254,14);
   vertex(224,26);
   vertex(227,34);
   vertex(190,54);
   endShape();
   ellipse(177,58,47,47);
   
   //Shaded areas
   
   //Slight shade under wings
   fill(0);
   ellipse(97,210,150,150);
   ellipse(301,210,150,150);
   
   translate(199,175);
   
   //Bottom left shade under star
   beginShape();
   vertex(-131,116);
   vertex(-54,135);
   vertex(0,191);
   vertex(0,1);
   vertex(-110,40);
   endShape();
   
   //Bottom right shade under star
   beginShape();
   vertex(0,192);
   vertex(56,135);
   vertex(133,116);
   vertex(110,40);
   vertex(0,1);
   endShape();
   
   translate(-199,-175);
   
   //wings
   fill(255,255,255);
   stroke(0);
   strokeWeight(13);
   line(112,112,20,74);
   line(286,112,377,74);
   arc(112,112,202,200,radians(140),radians(200));
   arc(120,177,208,208,radians(130),radians(185));
   arc(286,112,202,200,radians(-20),radians(40));
   arc(278,177,208,208,radians(-5),radians(50));
   
   //fill balnk spot
   noStroke();
   rect(42,140,314,50);
   
   //shift origin
   translate(199,175);
   
   //star
   stroke(0);
   strokeWeight(12);
   noFill();
   beginShape();
   vertex(0,-159);
   vertex(-58,-100);
   vertex(-139,-80);
   vertex(-116,0);
   vertex(-139,80);
   vertex(-58,100);
   vertex(0,159);
   vertex(58,100);
   vertex(139,80);
   vertex(116,0);
   vertex(139,-80);
   vertex(58,-100);
