   fill(255,225,91);
   beginShape();
   vertex(-131,-75);
   vertex(-109,0);
   vertex(-131,76);
   vertex(0,1);
   endShape();
   
   //Bottom left part
   fill(255,180,31);
   beginShape();
   vertex(-131,76);
   vertex(-54,95);
   vertex(0,151);
   vertex(0,1);
   endShape();
   
   //Bottom right part
   fill(200,93,58);
   beginShape();
   vertex(0,152);
   vertex(56,95);
   vertex(133,76);
   vertex(0,1);
   endShape();
   
   //Right part
   fill(255,180,31);
   beginShape();
   vertex(133,76  );
   vertex(110,0);
   vertex(132,-75);
   vertex(0,1);
   endShape();
   
   //Top right part
   fill(255,225,91);
   beginShape();
   vertex(132,-75);
   vertex(55,-94);
   vertex(0,-150);
   vertex(0,1);
   endShape();
 
 
   translate(-199,-175);
     
   //middle yellow circle shade
   fill(255,225,91);
   ellipse(199,176,188,188);
   
   //middle black circle
   fill(0,0,0);
   stroke(0,0,0);
   ellipse(199,172,172,168);
   
   //middle face
   fill(255,190,34);
   stroke(255,190,34);
   arc(199,171,142,136,radians(180),radians(360));
   arc(199,170,142,118,0,PI);
   rect(165,200,68,37,5);
   
   //eyes
   fill(0,0,0);
   noStroke();
   ellipse(166,187,44.5,46);
   ellipse(235,187,44.5,46);
   
   //left eyebrows
   rotate(radians(44));
   fill(0,0,0);
   rect(239,-17.5,14,10.5);
   ellipse(239,-12.25,10.5,10.5);
   ellipse(253,-12.25,10.5,10.5);
   
   //right eyebrows
   rotate(radians(1));
   rect(261,-44,10.5,14);
   ellipse(266.25,-44,10.5,10.5);
   ellipse(266.25,-30,10.5,10.5);
     
   //light spot on head
   rotate(radians(12));
   fill(255,255,255);
   ellipse(201,-68,36,54);
  
   //reset rotation to 0
   rotate(radians(-57));
   
   //nose
   fill(0);
   triangle(200,195,188,215,212,215);
   
   
}
